	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.AdministratorToStringConverter" /> 
		<bean class="converters.StringToAdministratorConverter" /> 
		
		<bean class="converters.SocialIdentityToStringConverter" /> 
		<bean class="converters.StringToSocialIdentityConverter" /> 
		
		<bean class="converters.RangerToStringConverter" /> 
		<bean class="converters.StringToRangerConverter" /> 
		
		<bean class="converters.ManagerToStringConverter" /> 
		<bean class="converters.StringToManagerConverter" /> 
		
		
		
		<bean class="converters.ContactEmergencyToStringConverter" /> 
		<bean class="converters.StringToContactEmergencyConverter" /> 
			
		
		<bean class="converters.ApplicationForToStringConverter" /> 
		<bean class="converters.StringToApplicationForConverter" /> 
		
		<bean class="converters.TagToStringConverter" /> 
		<bean class="converters.StringToTagConverter" /> 
		
		<bean class="converters.LegalTextToStringConverter" /> 
		<bean class="converters.StringToLegalTextConverter" /> 
		
		<bean class="converters.StageToStringConverter" /> 
		<bean class="converters.StringToStageConverter" /> 
		
		<bean class="converters.TripToStringConverter" /> 
